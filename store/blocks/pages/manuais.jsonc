    }
  },
  "link#baixar-manual-vida-75l-vertical":{
    "props": {
      "blockClass": "link-manuais",
      "href": "http://stermax.com.br/wp-content/uploads/2025/02/MANUAL-VIDA75L-VERTICAL_Rev-14.02.2025-compactado.pdf",
      "target": "_blank",
      "rel": "noopener noreferrer",
